<h1>Hi world</h1>
<form id="uploadForm" enctype="multipart/form-data" action="/song" method="post">
  <input type="text" name="name" required/>
  <input type="text" name="artist" required/>
  <input type="file" name="midi" required/>
  <input type="file" name="art" />
  <input type="submit" value="Upload Song" name="submit">
</form>
<div id="container">
  <div id="header">header</div>
  <div id="left-content">
    {{#each songs}}
      {{> song}}
    {{/each}}
  </div>
  <div id="right-content">
    <div id="now-playing-wrapper">
      <h1>now playing</h1>
    </div>
    <div id="song-queue-wrapper">
      {{> songQueue }}
    </div>
  </div>
</div>
