<div id="container">
  <form id="uploadForm" enctype="multipart/form-data" action="/song" method="post">
    Title: <input type="text" name="name" required/>
    Artist: <input type="text" name="artist" required/>
    Midi: <input type="file" name="midi" required/>
    <input type="submit" value="Upload Song" name="submit">
  </form>
</div>
